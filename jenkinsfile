pipeline{
 agent any
  tools{
    maven 'Maven'
  }
stages{
 stage ('Build') {
      steps {
      sh 'mvn clean package'
       }
    }
 stage('snyk') {
      steps {
        echo 'Testing...'
        snykSecurity(
          snykInstallation: 'snyk',
          snykTokenId: 'snyk',
          failOnIssues:'false'
          )
       }
     }
}
}
